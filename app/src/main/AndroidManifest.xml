<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cegeproommatefinder">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-feature android:name="android.hardware.type.watch" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".gagan.AnonymousActivity"></activity>
        <activity android:name=".CegepActivity" />
        <activity
            android:name=".FaqActivity"
            android:label="@string/title_activity_faq"
            android:parentActivityName=".gagan.MainActivity"></activity>
        <activity android:name=".gagan.NewPostActivity" />
        <activity android:name=".gagan.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ComplaintActivity"
            android:label="@string/title_activity_complaint"
            android:parentActivityName=".gagan.MainActivity" />
        <activity android:name=".gagan.RegisterActivity" />
        <activity android:name=".gagan.LoginActivity" />
        <activity
            android:name=".gagan.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Schedule" />
    </application>

</manifest>